{
  "name": "wheel-strat",
  "version": "1.54.7",
  "private": true,
  "main": "expo-router/entry",
  "workspaces": [
    "packages/*",
    "functions"
  ],
  "scripts": {
    "start": "EXPO_UNSTABLE_MCP_SERVER=1 npx expo start",
    "dev:bridge": "python3 scripts/bridge/ibkr_bridge.py",
    "dev:env": "node scripts/dev/dev_env.js",
    "data:refresh-historical": "node scripts/data/fetch_data.js",
    "ngrok": "ngrok http 5050",
    "db:proxy": "cloud-sql-proxy wheel-strat-483102:us-central1:historical --port 5432",
    "android": "expo run:android",
    "prebuild:ios": "npx expo prebuild -p ios --clean",
    "ios": "expo run:ios",
    "build:ios:dev": "eas build --local -p ios --profile development",
    "build:ios:preview": "eas build -p ios --profile preview",
    "build:ios:prod": "eas build -p ios --profile production --non-interactive",
    "update:ios": "eas update -p ios --channel production",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage --watchAll=false --watchman=false",
    "e2e:ios:build": "npx detox build -c ios.sim.debug",
    "e2e:ios:launch": "xcrun simctl install booted ios/build/Build/Products/Debug-iphonesimulator/WheelStrat.app && xcrun simctl launch booted art.dart.wheelstrat",
    "e2e:ios:test": "npx detox test -c ios.sim.debug",
    "e2e:android:build": "npx detox build -c android.emu.debug",
    "e2e:android:test": "npx detox test -c android.emu.debug",
    "doctor": "npx expo-doctor",
    "lint": "expo lint",
    "typecheck": "npx tsc --noEmit",
    "validate:ibkr-env": "node scripts/diagnostics/validate_ibkr_env.js",
    "validate:ibkr-env:strict": "node scripts/diagnostics/validate_ibkr_env.js --strict"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/analytics": "^23.8.6",
    "@react-native-firebase/app": "^23.8.6",
    "@react-native-firebase/auth": "^23.8.6",
    "@react-native-firebase/crashlytics": "^23.8.6",
    "@react-native-firebase/firestore": "^23.8.6",
    "@react-native-firebase/functions": "^23.8.6",
    "@react-native-google-signin/google-signin": "^16.1.1",
    "@react-native-segmented-control/segmented-control": "2.5.7",
    "@shopify/flash-list": "2.0.2",
    "@shopify/react-native-skia": "2.2.12",
    "@wheel-strat/shared": "1.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "d3-array": "^3.2.4",
    "d3-color": "^3.1.0",
    "d3-dispatch": "^3.0.1",
    "d3-drag": "^3.0.0",
    "d3-ease": "^3.0.1",
    "d3-format": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-path": "^3.1.0",
    "d3-quadtree": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "d3-timer": "^3.0.1",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "expo": "~54.0.33",
    "expo-apple-authentication": "~8.0.8",
    "expo-blur": "^15.0.8",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-glass-effect": "~0.1.8",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-insights": "~0.10.8",
    "expo-keep-awake": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.15",
    "expo-video": "~3.0.15",
    "firebase": "^12.8.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^9.7.6",
    "react-native-purchases-ui": "^9.7.6",
    "react-native-reanimated": "~4.1.1",
    "react-native-reanimated-carousel": "^4.0.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "tinybase": "7.0.0",
    "victory-native": "^41.20.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/runtime": "^7.28.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/d3-array": "^3",
    "@types/d3-color": "^3",
    "@types/d3-dispatch": "^3",
    "@types/d3-drag": "^3",
    "@types/d3-ease": "^3",
    "@types/d3-format": "^3",
    "@types/d3-interpolate": "^3",
    "@types/d3-path": "^3",
    "@types/d3-quadtree": "^3",
    "@types/d3-scale": "^4",
    "@types/d3-selection": "^3",
    "@types/d3-shape": "^3",
    "@types/d3-time": "^3",
    "@types/d3-time-format": "^4",
    "@types/d3-timer": "^3",
    "@types/d3-transition": "^3",
    "@types/d3-zoom": "^3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18",
    "detox": "^20.47.0",
    "detox-expo-helpers": "^0.6.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "expo-detox-hook": "^1.0.10",
    "expo-mcp": "~0.2.1",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.17",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|tinybase|firebase|@firebase|victory-native)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/functions/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "moduleNameMapper": {
      "victory-native": "<rootDir>/__mocks__/victory-native.js",
      "^@/(.*)$": "<rootDir>/$1",
      "^@firebase/util$": "<rootDir>/node_modules/@firebase/util/dist/index.cjs.js",
      "^@wheel-strat/shared$": "<rootDir>/packages/shared/src/index.ts",
      "^@wheel-strat/shared/(.*)$": "<rootDir>/packages/shared/src/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 55,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "resolutions": {
    "d3-color": "3.1.0",
    "d3-interpolate": "3.0.1",
    "d3-zoom": "3.0.0"
  }
}